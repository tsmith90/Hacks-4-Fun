#!/usr/bin/python
import subprocess
from subprocess import *

nrange = "10.11.1."

for i in range(0, 254):
	address = nrange + str(i)
	res = subprocess.call(['ping', '-c', '3', address])
	if res == 0:
		print "ping to", address, "OK"
	elif res == 2:
		print "no response from", address
	else:
		print "ping to", address, "failed!"

